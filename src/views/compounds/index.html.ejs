<div class="center">
  <h1>Recintos</h1>

  <% if (hasCreatePermission) { %>
    <form action="<%= newCompoundPath %>">
        <input type="submit" value="Crear recinto"/>
    </form>
  <% } %>

  <ul>
    <% compounds.forEach((compound) => { %>
      <li>
          <a class="lilink" href=<%= getCompoundPath(compound) %>>
          <img class="medium-logo" src="<%= compound.photo %>"/>
          <div>
           <%= compound.name %>
         </div>
       </a>
      </li>
    <% }) %>
  </ul>
</div>
