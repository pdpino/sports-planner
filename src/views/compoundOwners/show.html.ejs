<h1><%= compoundOwner.firstName %> <%= compoundOwner.lastName %></h1>

<div>
  <h4>Información personal</h4>
  <img src=<%= compoundOwner.photo %> > <br>

  <p> E-mail: <%= compoundOwner.email %> </p>

  <br>


  <p> Teléfono: <%= compoundOwner.phone %> </p>

  <form action="<%= editcompoundOwnerPath %>">
      <input type="submit" value="Editar Información personal" />
  </form>
</div>

<% if(compounds.length > 0) { %>
<ul>
  <% compounds.forEach((compound) => { %>
    <li>
        <%= compound.name %>
        <form action=<%= compoundPath(compound) %>>
            <input type="submit" value="Ver recinto"/>
        </form>
    </li>
  <% }) %>
</ul>
<% } %>

<br />

<form action="<%= compoundOwnersPath %>">
    <input type="submit" value="Volver" />
</form>
