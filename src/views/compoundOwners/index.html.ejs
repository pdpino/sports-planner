<h1>Compound Owners en Jugamos</h1>

<ul>
  <% compoundOwners.forEach((compoundOwner) => { %>
    <li>
        <%= compoundOwner.email %>
        <form action=<%= getCompoundOwnerPath(compoundOwner) %>>
            <input type="submit" value="Ver perfil"/>
        </form>
    </li>
  <% }) %>
</ul>
