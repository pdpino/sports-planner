<h1>Equipos en jugamos</h1>

<form action=<%= newTeamPath %>>
    <input type="submit" value="Crear Equipo"/>
</form>

<ul>
  <% teams.forEach((team) => { %>
    <li>
      <a href="<%= teamPath(team) %>">
        <%= team.name %>
      </a>
      <p>
        Logo: <%= team.logo %>
        <br>
        Deporte: <%= getTeamSport(team) %>
      </p>
    </li>
  <% }) %>
</ul>
