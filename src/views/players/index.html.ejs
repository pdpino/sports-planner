<div class ="center">
<h1>Players en Jugamos</h1>

<ul>
  <% players.forEach((player) => { %>

    <li>
        <a class="lilink" href=<%= playerPath(player) %>>
        <img src=<%=player.photo%> width="128" height="128"/>
        <div>
         <%= player.firstName %> <%= player.lastName %>
       </div>
       <div>
        <%= player.email %>
        </div>
      </a>
    </li>
  <% }) %>
</ul>
</div>
