<div class="center">
<h1>Players en Jugamos</h1>

<ul>
  <% players.forEach((player) => { %>
    <li>
      <a class="lilink" href="<%= getPlayerPath(player) %>">
        <img src="<%=player.photo%>" class="large-logo"/>
        <div>
          <%= player.firstName %> <%= player.lastName %>
        </div>
        <div>
          <%= player.email %>
        </div>
      </a>
    </li>
  <% }) %>
</ul>
</div>
