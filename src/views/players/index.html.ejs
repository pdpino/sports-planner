<h1>Players en Jugamos</h1>

<form action=<%= newPlayerPath %>>
    <input type="submit" value="Crear Jugador"/>
</form>

<ul>
  <% players.forEach((player) => { %>
    <li>
      <!-- <a href="<%=  %>"> -->

        <%= player.email %>
        <form action=<%= playerPath(player) %>>
            <input type="submit" value="Ver perfil"/>
        </form>
      <!-- </a> -->
    </li>
  <% }) %>
</ul>
